<resources>
    <!-- Base application theme. -->
    <style name="Base.Theme.FiveMPK" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- Primary color. -->
        <item name="colorPrimary">#006493</item>
        <item name="colorOnPrimary">#ffffff</item>
        <item name="colorPrimaryContainer">#cae6ff</item>
        <item name="colorOnPrimaryContainer">#001e30</item>
        <!-- Secondary color. -->
        <item name="colorSecondary">#50606e</item>
        <item name="colorOnSecondary">#ffffff</item>
        <item name="colorSecondaryContainer">#d3e5f5</item>
        <item name="colorOnSecondaryContainer">#0c1d29</item>
        <!-- Tertiary color -->
        <item name="colorTertiary">#00639a</item>
        <item name="colorOnTertiary">#ffffff</item>
        <item name="colorTertiaryContainer">#cde5ff</item>
        <item name="colorOnTertiaryContainer">#001d32</item>
        <item name="android:textColorTertiary">#00639a</item>
        <!--Error color -->
        <item name="colorError">#ba1a1a</item>
        <item name="colorOnError">#ffffff</item>
        <item name="colorErrorContainer">#ffdad6</item>
        <item name="colorOnErrorContainer">#410002</item>
        <!-- Background color -->
        <item name="backgroundColor">#f8fdff</item>
        <item name="colorOnBackground">#001f25</item>
        <item name="colorSurface">#6BAAFF</item>
        <item name="colorOnSurface">#001f25</item>
        <!-- Outline color -->
        <item name="colorOutline">#72787e</item>
        <item name="colorSurfaceVariant">#dde3ea</item>
        <item name="colorOnSurfaceVariant">#41474d</item>
        <!--Card color-->
        <item name="cardBackgroundColor">#EBEBEB</item>
        <!--SearchBar Color-->
        <item name="colorContainer">#ffffff</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Font Family -->
        <item name="fontFamily">@font/poppinsmedium</item>

    </style>
    <style name="Theme.FiveMPK" parent="Base.Theme.FiveMPK" />
    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="android:windowBackground">@color/black</item>
        <item name="android:windowFullscreen">true</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/animated_logo</item>
        <item name="postSplashScreenTheme">@style/Base.Theme.FiveMPK</item>
        <!-- Add other customizations as needed -->
        <item name="windowSplashScreenAnimationDuration">200</item>
    </style>
</resources>